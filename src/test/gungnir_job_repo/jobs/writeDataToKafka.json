{
  "name": "writeDataToKafka",
  "queryPlan":[
    [
      {
        "queryName":"readFromFile",
        "parameters":{
          "input": "conv",
          "timeStampField": "@timestamp",
          "schemaName": "conv",
          "format": "json",
          "dataLocation": "src/test/gungnir_job_repo/testData/",
          "output": "convTest"
        }
