CREATE TEMP_VIEW TEMP AS SELECT endOfDay(time_stamp) as time_stamp,
       orgId,
       userId,
       isMessage,
       isCall
FROM  SOURCE_VIEW;

SELECT time_stamp,
       convertTime(time_stamp) as pdate,
       CONCAT(convertTime(time_stamp), '^', orgId, '^', userId) as dataid,
       orgId,
       userId,
       sum(isMessage) AS isMessage,
       sum(isCall) AS isCall,
       'activeUserRollUp' AS relation_name
FROM TEMP
GROUP BY orgId,
         userId,
         time_stamp