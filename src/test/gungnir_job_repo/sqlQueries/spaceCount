CREATE TEMP_VIEW TEMP AS SELECT aggregateStartDate(time_stamp) as time_stamp,
    orgId,
    group
FROM  SOURCE_VIEW;

CREATE TEMP_VIEW TEMP1 AS DropDuplicates time_stamp, orgId, group;

SELECT CONCAT(time_stamp, '^', orgId, '^', 'activeUser', '^', periodTag(orgId)) AS eventKey,
       time_stamp,
       orgId,
       COUNT(group) AS spaceCount,
       periodTag(orgId) AS period,
       'activeUser' AS relation_name
FROM TEMP1
GROUP BY orgId,
         time_stamp